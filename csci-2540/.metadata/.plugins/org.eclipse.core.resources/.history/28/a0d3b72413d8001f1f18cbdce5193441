package assg1_fitchj23;

import java.util.Scanner;

public class DateChange {
	public static void main(String[] args) {
		
		Scanner kbd = new Scanner(System.in);
		String usDate;
		String euDate;
		
		System.out.print("Enter a date in the form mon/day/year: ");
		usDate = kbd.nextLine();
		
		euDate = usDate.substing(usDate.indexOf("/"),usDate.lastIndexOf("/"));
		
		System.out.print(euDate);
	}

}
